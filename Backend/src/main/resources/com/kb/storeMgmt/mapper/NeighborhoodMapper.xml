<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.kb.storeMgmt.mapper.NeighborhoodMapper">
    <select id="findAddressById" resultType="com.kb.storeMgmt.dto.NeighborhoodDTO">
        SELECT *
        FROM mystore
        WHERE id = #{id}
    </select>
</mapper>
